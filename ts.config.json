{
  "compilerOptions" : {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "lib": ["ES2020"],
    "resolveJsonModule": true
  }
}
